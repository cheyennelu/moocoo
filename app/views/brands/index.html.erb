<div class="brand-container">
  <%# brand_name = ["dior.jpg", "Lancome.jpg", "Loreal.jpg"] %>
  <% @brands.each do |brand| %>
    <div class="row">
      <div class="col-12">
        <%= link_to brand_path(brand.id) do %>
        <%= image_tag brand.image_file_path %>
        <% end %>
      </div>
    </div>
  <% end  %>
</div>
