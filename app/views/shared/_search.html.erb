<%= form_with url: "/search", method: :get do |form| %>
  <%= form.label :q, "Search for:" %>
  <%= form.text_field :q %>
  <%= form.radio_button :product, "Products" %>
  <%= form.label :product, "Products" %>
  <%= form.radio_button :video, "Videos" %>
  <%= form.label :video, "Videos" %>
  <%= form.submit "Search", class: 'btn btn-primary' %>
<% end %>


